            </span>
    </p>
    </div>

    <script>
        const expand = document.querySelector('.expand')
        const explain = document.querySelector('.explain')
        const explainShopify = document.querySelector('.explain-shopify')
        const closes = document.querySelector('.close')

        explain.addEventListener('click', function(){
            explainShopify.removeAttribute('hidden')
            explain.style.display = 'none'
            closes.removeAttribute('hidden')
        })
        closes.addEventListener('click', function(){
            explainShopify.setAttribute('hidden','')
            explain.style.display = 'block'
            closes.setAttribute('hidden','')
        })

        const expandCost = document.querySelector('.expand-cost')
        const explainCost = document.querySelector('.explain-cost')
        const explainShopifyCost = document.querySelector('.explain-shopify-cost')
        const closesCost = document.querySelector('.close-cost')

        explainCost.addEventListener('click', function(){
            explainShopifyCost.removeAttribute('hidden')
            explainCost.style.display = 'none'
            closesCost.removeAttribute('hidden')
        })
        closesCost.addEventListener('click', function(){
            explainShopifyCost.setAttribute('hidden','')
            explainCost.style.display = 'block'
            closesCost.setAttribute('hidden','')
        })

    </script>



    